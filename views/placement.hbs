}
.menu-bar .navbar-toggler{
padding-right: 0;
border: none;
outline: none;
}
.banner{
    height: 80vh;
    background-image: linear-gradient(rgba(0,0,0,0.5),rgba(0,0,0,0.5)),url("https://images.shiksha.com/mediadata/images/1509627375phpPbeawb.jpeg");
