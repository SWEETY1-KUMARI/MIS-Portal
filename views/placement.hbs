  .experience-box,
.comment-box {
  border: 1px solid #ccc;
  padding: 10px; /* Reduce padding */
  margin: 10px;
  text-align: center;
  transition: box-shadow 0.3s, transform 0.3s;
  border-radius: 10px; /* Add border-radius for a rounded appearance */
}

.experience-box img,
.comment-box img {
  max-width: 50px; /* Reduce image size */
  height: 50px; /* Set a fixed height for circular images */
  border-radius: 50%; /* Make the images circular */
  margin-bottom: 10px;
}

.experience-box:hover,
.comment-box:hover {
  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1); /* Add box shadow on hover */
  transform: translateY(-5px); /* Add a slight lift effect on hover */
}

