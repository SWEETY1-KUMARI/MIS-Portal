                <label for="comment">Write your comment:</label>
                <textarea class="form-control" id="comment" rows="3"></textarea>
              </div>
              <div class="mb-3">
                <label for="photo">Upload your photo:</label>
                <input type="file" class="form-control" id="photo" accept="image/*">
              </div>
              <button type="button" class="btn btn-primary" onclick="submitComment()">Submit Comment</button>
            </form>
          </div>
        </div>
      </div>


      <script>
        function submitComment() {
          var commentText = document.getElementById("comment").value;
          var photoInput = document.getElementById("photo");
          var photoUrl = "";
      
          if (photoInput.files.length > 0) {
            var photo = photoInput.files[0];
            photoUrl = URL.createObjectURL(photo);
          }
      
          var commentContainer = document.createElement("div");
          commentContainer.classList.add("col-md-4", "experience-box"); // Use the same style as experience-box
      
          if (photoUrl !== "") {
            var photoElement = document.createElement("img");
            photoElement.src = photoUrl;
            photoElement.style.maxWidth = "100%"; // Ensure the image doesn't exceed the container width
            commentContainer.appendChild(photoElement);
          }
      
          var commentParagraph = document.createElement("p");
          commentParagraph.textContent = commentText;
          commentContainer.appendChild(commentParagraph);
      
          var commentsRow = document.getElementById("commentsRow");
          var commentRow = commentsRow.lastElementChild;
      
          // Check if the current row is undefined or already contains 3 elements
          if (!commentRow || commentRow.childElementCount === 3) {
            commentRow = document.createElement("div");
            commentRow.classList.add("row");
            commentsRow.appendChild(commentRow);
          }
      
          commentRow.appendChild(commentContainer);
      
          // Clear form fields
          document.getElementById("commentForm").reset();
        }
      </script>
      

    
    <script src="https://code.jquery.com/jquery-3.6.0.min.js" ></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.min.js" ></script>
</body>
</html>
